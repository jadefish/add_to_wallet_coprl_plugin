{
  "compilerOptions": {
    "incremental": true,
    "target": "es5",
    "isolatedModules": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": true
  },
  "include": ["src/**/*"]
}
